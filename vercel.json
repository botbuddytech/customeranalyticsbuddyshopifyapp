{
  "buildCommand": "npm run setup && npm run build",
  "outputDirectory": "build/client",
  "installCommand": "npm install",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "includeFiles": "build/**"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/index.js" }
  ]
}
