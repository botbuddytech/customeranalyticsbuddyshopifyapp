{
  "buildCommand": "npm run setup && npm run build",
  "outputDirectory": "build/client",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "build/server/**",
      "maxDuration": 30
    }
  }
}

